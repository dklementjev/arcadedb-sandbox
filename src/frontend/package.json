{
  "name": "graph-sandbox-frontend",
  "packageManager": "yarn@4.9.2",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.0",
    "graphology": "^0.26.0",
    "graphology-layout": "^0.6.1",
    "graphology-layout-force": "^0.2.4",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-layout-noverlap": "^0.4.2",
    "graphology-types": "^0.24.8",
    "mitt": "^3.0.1",
    "sigma": "^3.0.2"
  },
  "scripts": {
    "build-app-bundle": "node ./esbuildfile.mjs",
    "build-bootstrap": "install -m 0644 -Dv -t public/assets/bootstrap node_modules/bootstrap/dist/css/bootstrap.css node_modules/bootstrap/dist/js/bootstrap.js",
    "build-vendor": "yarn run build-bootstrap",
    "build": "yarn run build-vendor && yarn run build-app-bundle",
    "lint": "tsc --noEmit"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "less": "^4.5.1"
  }
}
